{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Agency CLI",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/agency.py",
      "console": "integratedTerminal",
      "envFile": "${workspaceFolder}/.env",
      "env": {
        "AGENCY_FORCE_TERMINAL": "1"
      },
      "justMyCode": false
    },
    {
      "name": "Node: Test Notion MCP",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/tmp/fetch_notion_session.mjs",
      "console": "integratedTerminal",
      "env": {
        "MCP_REMOTE_CONFIG_DIR": "${workspaceFolder}/mcp_credentials"
      }
    }
  ]
}
